package kr.co.findourlunch.menu;

import java.util.List;

public class SearchVO {
	
	private List<ValueCodeVO> valueList;
	
	private List<String> searchWordList;

	private List<MenuVO> menuList;
	
	private List<RestaurantVO> restaurantList;
	
	private int id;
	
	private int menu_id;
	
	private int cate_id;
	
	/**
	 * MENUDATA.value_id
	 */
	private int value_id;

	/**
	 * MENU.name
	 */
	private String menu_name;
	
	public List<ValueCodeVO> getValueList() {
		return valueList;
	}

	public void setValueList(List<ValueCodeVO> valueList) {
		this.valueList = valueList;
	}

	public List<String> getSearchWordList() {
		return searchWordList;
	}

	public void setSearchWordList(List<String> searchWordList) {
		this.searchWordList = searchWordList;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getMenu_id() {
		return menu_id;
	}

	public void setMenu_id(int menu_id) {
		this.menu_id = menu_id;
	}

	public int getCate_id() {
		return cate_id;
	}

	public void setCate_id(int cate_id) {
		this.cate_id = cate_id;
	}

	public int getValue_id() {
		return value_id;
	}

	public void setValue_id(int value_id) {
		this.value_id = value_id;
	}

	public String getMenu_name() {
		return menu_name;
	}

	public void setMenu_name(String menu_name) {
		this.menu_name = menu_name;
	}

	public List<MenuVO> getMenuList() {
		return menuList;
	}

	public void setMenuList(List<MenuVO> menuList) {
		this.menuList = menuList;
	}

	public List<RestaurantVO> getRestaurantList() {
		return restaurantList;
	}

	public void setRestaurantList(List<RestaurantVO> restaurantList) {
		this.restaurantList = restaurantList;
	}

	
	
}
