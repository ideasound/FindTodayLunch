package kr.co.findourlunch.menu;

public class ValueCodeVO {

	
	private int value_id;
	
	private String value;
	
	private int ref_cate_id;

	public int getValue_id() {
		return value_id;
	}

	public void setValue_id(int value_id) {
		this.value_id = value_id;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}

	public int getRef_cate_id() {
		return ref_cate_id;
	}

	public void setRef_cate_id(int ref_cate_id) {
		this.ref_cate_id = ref_cate_id;
	}

	
	
}
